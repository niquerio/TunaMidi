<form class="form-inline">
  <div class="form-group">
    <label class="col-sm-6" >Current Measure: </label>
    <div class="col-sm-6">
      <p class="form-control-static" id="currentMeasure"><%= currentMeasure %></p>
    </div>
  </div>
<div class="form-group">
  <label class="col-sm-6">Measure Range: </label>
  <div class="col-sm-6">
  <p class="form-control-static" id="measureRange"><%= start %> - <%= finish %></p>
  </div>
</div> 
<div data-slider-id="measuresSlider" class="set_of_measures"></div>
</form>
